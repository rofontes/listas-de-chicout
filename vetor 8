#include <stdio.h>
#define tam 6

int main() {
   int vet[tam], i,ma = 0, me = 0, pma,pme;
   
   printf("⬇ Digite seu vetor ⬇\n");
   for (i = 0; i < tam; i++) {
     printf("-> ");
       scanf ("%i", &vet[i]);
   }
  
   printf("Vetor:");
   for (i = 0; i < tam; i++) {
       printf ("[%i]", vet[i]);
   }
   printf("\nInverso:");
   for (i = tam; i > 0; i--) {
       printf ("[%i]", vet[i-1]);
   }    
    return 0;
}
